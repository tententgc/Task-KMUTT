   "source": [
    "import math\n",
    "\n",
    "def f(x): return x**3  - 2\n",
    "\n",
    "def g(x): return x**3 + x - 2\n",
    "\n",
    "\n",
    "def fixedPoint(x0, iteration=5):\n",
    "    i = 0\n",
    "    while i < iteration:\n",
    "        x1 = g(x0)\n",
    "        print(f\"{i} - {x1:.5f}\")\n",
    "        x0 = x1\n",
    "        i += 1\n",
    "\n",
    "\n",
    "\n",
    "fixedPoint(1.2)"
   ]
  }
